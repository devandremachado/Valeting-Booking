<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - Valeting.Web</title>

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;700&display=swap" rel="stylesheet">
    <link href="~/fonts/FontAwesomeFree_5.13/css/all.min.css" rel="stylesheet">

    <!-- Plugins CSS -->
    <link rel="stylesheet" href="~/lib/Bootstrap_4.4.1/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/JqueryUI-1.12.1/css/jquery-ui.min.css" />

    <link rel="stylesheet" href="~/lib/Datepicker/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/lib/Animate/css/animate.min.css" />
    <link rel="stylesheet" href="~/lib/JQueryDataTables_1.10.21/css/datatables.min.css" />
    <link rel="stylesheet" href="~/lib/JQueryDataTables_1.10.21/Responsive-2.2.5/css/responsive.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/JQuerySelect2/css/select2.min.css" />
    <link rel="stylesheet" href="~/lib/JQuerySelect2/css/select2-bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/SweetAlert2-9.15.1/css/sweetalert2.min.css" />

    <link rel="stylesheet" href="~/shared/css/Utility.min.css" />
    <link rel="stylesheet" href="~/shared/css/style.min.css" />

    <link rel="stylesheet" href="~/admin/css/layout/Layout.min.css" />
</head>

<body>
    <header>
        <div class="container">
            <div class="box-header">

                <div class="left-header">
                    <div class="box-info-usuario">
                        <div class="info-extra">
                            <div class="info-links">
                                <a href="@Href("~/Admin/Home")">Home</a>
                                <div class="separador"></div>
                                <a href="@Href("~/Admin/Login")">Log out</a>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="center-header"></div>

                <div class="right-header">
                    <nav id="navMenu" class="navMenu">
                        <ul>
                            <li><a href="javascript:void(0)" id="btnFecharMenu" class="btnFecharMenu">&times;</a></li>
                            <li><a href="@Href("~/Admin/Home")">Home</a></li>
                            <li><a href="@Href("~/Admin/CarBooking")">Register Booking</a></li>
                            <li><a href="@Href("~/Admin/Login")">Log out</a></li>
                        </ul>
                    </nav>

                    <span id="btnAbrirMenu" style="font-size: 30px; cursor: pointer">&#9776;</span>
                </div>
            </div>

        </div>
    </header>

    <main role="main" class="animated fadeInDown">
        @RenderBody()
    </main>

    <footer>
        <div class="container">
            <div class="box-footer">
            </div>
        </div>
    </footer>

    <script type="text/javascript">
        var Master_Vars = {
            baseUrl: '@Href("~/")'
        };
    </script>

    <!-- Plugins JS -->
    <script src="~/lib/JQuery_3.5.1/js/jquery-3.5.1.min.js"></script>
    <script src="~/lib/JQueryUI-1.12.1/js/jquery-ui.min.js"></script>
    <script src="~/lib/Popper/js/popper.min.js"></script>
    <script src="~/lib/Bootstrap_4.4.1/js/bootstrap.min.js"></script>

    <script src="~/lib/BlockUI/js/jquery.blockUI.js"></script>
    <script src="~/lib/Datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="~/lib/JQueryInputMask/js/jquery.inputmask.min.js"></script>
    <script src="~/lib/JQueryValidation_1.19.2/js/jquery.validate.min.js"></script>
    <script src="~/lib/JQueryValidation_1.19.2/js/additional-methods.min.js"></script>
    <script src="~/lib/JQueryDataTables_1.10.21/js/datatables.min.js"></script>
    <script src="~/lib/JQueryDataTables_1.10.21/Responsive-2.2.5/js/dataTables.responsive.min.js"></script>
    <script src="~/lib/JQuerySelect2/js/select2.min.js"></script>
    <script src="~/lib/SweetAlert2-9.15.1/js/sweetalert2.all.min.js"></script>

    <script src="~/shared/js/utility.js"></script>

    <script src="~/admin/js/layout/layout.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
